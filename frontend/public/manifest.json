{
    "manifest_version": 3,
    "name": "CodeLab Due Date Tracker",
    "description": "Tracks User Due Dates",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "css": ["popup.css"]
        }
    ],
    "permissions": [
        "identity"
    ],
    "content-security-policy": "default-src 'self'; report-uri https://www.example.com/cspreport"
}